# Arcade.dev Integration Examples - Environment Configuration
# Copy this file to .env and configure with your actual values

# Required Configuration
# =====================

# Arcade.dev API Key (Required)
# Get this from your Arcade.dev dashboard
ARCADE_API_KEY=your_arcade_api_key_here

# Optional Configuration
# =====================

# Arcade.dev API URL (Optional)
# Default: https://api.arcade.dev
ARCADE_API_URL=https://api.arcade.dev

# Arcade.dev Request Timeout in seconds (Optional)
# Default: 30
ARCADE_TIMEOUT=30

# Arcade.dev Maximum Retries (Optional)
# Default: 3
ARCADE_MAX_RETRIES=3

# FACT Framework Log Level (Optional)
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
# Default: INFO
FACT_LOG_LEVEL=INFO

# FACT Cache Enable/Disable (Optional)
# Options: true, false
# Default: true
FACT_CACHE_ENABLED=true

# Additional Optional Configuration
# ================================

# Arcade.dev Workspace ID (Optional)
# Required for some advanced features
ARCADE_WORKSPACE_ID=your_workspace_id

# Redis Configuration for Caching (Optional)
# Required only if using Redis-based caching
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600

# Monitoring Configuration (Optional)
# Required only if using monitoring features
ENABLE_METRICS=true
METRICS_ENDPOINT=http://localhost:8080/metrics

# Security Configuration (Optional)
# Advanced security settings
ENABLE_AUDIT_LOGGING=true
MAX_REQUEST_SIZE=10485760
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Development Configuration (Optional)
# For development and debugging
DEBUG_MODE=false
VERBOSE_LOGGING=false
ENABLE_PROFILING=false